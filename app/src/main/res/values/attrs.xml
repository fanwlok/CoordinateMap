<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 带分割线的网格布局  inScrollView：是否放在可滚动的GroupView中 -->
    <declare-styleable name="MyGridView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="dividerColor" format="color" />
        <attr name="showButtonLine" format="boolean"/>
        <attr name="showTopLine" format="boolean"/>
        <attr name="inScrollView" format="boolean"/>
    </declare-styleable>
    <!--腾讯通信-->

    <!-- 条形控制控件自定义属性 -->
    <declare-styleable name="LineControllerView">
        <!-- 名称 -->
        <attr name="name" format="string"/>
        <!-- 内容或当前状态 -->
        <attr name="content" format="string"/>
        <!-- 是否是列表中最后一个 -->
        <attr name="isBottom" format="boolean"/>
        <!-- 是否可以跳转 -->
        <attr name="canNav" format="boolean"/>
        <!-- 是否是开关 -->
        <attr name="isSwitch" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ScrimInsetsView">
        <attr name="obaInsetForeground" format="reference|color"/>
    </declare-styleable>
</resources>