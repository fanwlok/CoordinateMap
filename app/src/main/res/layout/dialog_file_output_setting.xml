<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


        <CheckBox
            android:id="@+id/cb_point_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="名称" />

        <CheckBox
            android:id="@+id/cb_point_wgs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="wgs经纬度"/>


        <CheckBox
            android:id="@+id/cb_point_altitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="海拔"/>

        <CheckBox
            android:id="@+id/cb_point_bd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="百度经纬度"/>


        <CheckBox
            android:id="@+id/cb_point_describe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="描述" />

        <CheckBox
            android:id="@+id/cb_point_adress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地址"/>
    <CheckBox
        android:id="@+id/cb_point_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="照片"/>
</LinearLayout>