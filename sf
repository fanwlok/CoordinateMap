[32m+[m[32m            case STYLE_RED:[m
[32m+[m[32m            case STYLE_BLUE:[m
[32m+[m[32m            case STYLE_GREEN:[m
[32m+[m[32m            case STYLE_PURPLE:[m
[32m+[m[32m            case STYLE_ORANGE:[m
[32m+[m[32m                return R.style.Bubble_TextAppearance_Light;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
