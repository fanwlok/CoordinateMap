[32m+[m[32mpublic class DaoSession extends AbstractDaoSession {[m
[32m+[m
[32m+[m[32m    private final DaoConfig filesDaoConfig;[m
[32m+[m[32m    private final DaoConfig showDataDaoConfig;[m
