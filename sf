[32m+[m[32m        public int getChildrenCount(int groupPosition) {[m
[32m+[m[32m            Log.d("CITY", City.get(groupPosition));[m
[32m+[m[32m            return AllCity.get(City.get(groupPosition)).size();[m
[32m+[m[32m        }[m
[32m+[m
